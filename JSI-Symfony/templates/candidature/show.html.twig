{% extends 'backoffice.html.twig' %}

{% block title %}Recrutement{% endblock %}

{% block body %}
<div class="container">
    <h1>Candidature</h1>
     <table class="table content-panel">
        <tbody>
            <tr>
                <th>Nom</th>
                <td>{{ candidature.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ candidature.prenom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ candidature.email }}</td>
            </tr>
            <tr>
                <th>CV</th>
                <td>{{ candidature.cv }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ candidature.message }}</td>
            </tr>
        </tbody>
    </table>
    <a class="btn btn-primary" href="{{ path('candidature') }}">Retour</a>
</div>
{% endblock %}