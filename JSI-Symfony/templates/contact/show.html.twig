{% extends 'backoffice.html.twig' %}

{% block title %}Contact{% endblock %}

{% block body %}
<div class="container">
    <h1>Contact</h1>
    <table class="table content-panel">
        <tbody>
            <tr>
                <th>Nom</th>
                <td>{{ contact.nom }}</td>
            </tr>
            <tr>
                <th>Societe</th>
                <td>{{ contact.societe }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ contact.email }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ contact.telephone }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ contact.message }}</td>
            </tr>
            <tr>
                <th>Demande</th>
                <td>{{ contact.demande }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-primary" href="{{ path('contact_index') }}">Retour</a>
</div>
    {# <a href="{{ path('contact_edit', {'id': contact.id}) }}">edit</a> #}

    {# {{ include('contact/_delete_form.html.twig') }} #}
{% endblock %}
